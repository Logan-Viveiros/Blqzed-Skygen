{
	"$schema": "https://raw.githubusercontent.com/Bedrock-OSS/regolith-schemas/main/config/v1.6.json",
	"author": "iBlqzed",
	"name": "Blqzed Skygen",
	"packs": {
		"behaviorPack": "./packs/BP",
		"resourcePack": "./packs/RP"
	},
	"regolith": {
		"dataPath": "./packs/data",
		"filterDefinitions": {
			"generator": {
				"runWith": "nodejs",
				"script": "./filters/generator.js"
			}
		},
		"formatVersion": "1.6.0",
		"profiles": {
			"default": {
				"export": {
					"build": "standard",
					"readOnly": false,
					"target": "development"
				},
				"filters": [
					{
						"filter": "generator"
					}
				]
			}
		}
	}
}